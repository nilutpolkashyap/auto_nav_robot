<launch>

<rosparam file="$(find my_robot_description)/launch/controller.yaml" command="load"/>
<node args="motor_rf_position_controller motor_rb_position_controller motor_lb_position_controller motor_lf_position_controller motor_arm_position_controller motor_gripper_position_controller joint_state_controller " name="controller_spawner" ns="my_robot" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/my_robot/joint_states"/>
</node>

</launch>